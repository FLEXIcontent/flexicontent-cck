				<option value="0">FLEXI_NO</option>
				<option value="1">FLEXI_YES</option>
			</field>
			<field name="orderby_override" type="list" default="" label="FLEXI_ORDER_1ST" description="FLEXI_ORDER_1ST_DESC">
				<option value="">FLEXI_USE_GLOBAL</option>
