<?xml version="1.0" encoding="utf-8"?>
<form>
	<field
		name="id"
		type="hidden"
		default="0"
		label="JGLOBAL_FIELD_ID_LABEL"
		description ="JGLOBAL_FIELD_ID_DESC"
		class="readonly"
		readonly="true" />
	<field
		name="name"
		type="text"
		label="FLEXI_TYPE_NAME"
		description="JFIELD_TITLE_DESC"
		class="inputbox required"
		size="40"
		required="true" />
	<field
		name="alias"
		type="text"
		label="FLEXI_ALIAS"
		description="JFIELD_ALIAS_DESC"
		class="inputbox"
		size="40" />
	<field
		name="published"
		type="list"
		class="inputbox"
		default="1"
		size="1"
		label="FLEXI_PUBLISHED"
		description="JFIELD_PUBLISHED_DESC">
		<option
			value="1">
			JPUBLISHED</option>
		<option
			value="0">
			JUNPUBLISHED</option>
	</field>
	
	<field
		name="checked_out"
		type="hidden"
		filter="unset" />

	<field
		name="checked_out_time"
		type="hidden"
		filter="unset" />
	
	<field
		id="access"
		name="access"
		type="accesslevel"
		label="JField_Access_Label"
		description="JField_Access_Desc"
		class="inputbox"
		size="1"
		default="" />
