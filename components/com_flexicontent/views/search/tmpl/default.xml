				<option value="1">Advanced Index (Below fields)</option>
				<option value="2">Advanced Index (Below fields) + user selection</option>
			</field>
			<field name="advsearch_fieldlist" type="fields" default="" label="Select Field" description="Please select one by one fields to add to textarea below." fieldnameastext="true" fieldnameasvalue="true" isadvsearch="1" onchange="f=document.getElementById('{control_name}txtflds'); f.value = f.value ? f.value + ','+this.value : this.value; this.value='';" />
			<field name="txtflds" type="textarea" default="" label="Fields &lt;br/&gt; &lt;small&gt;(Only for Advanced Index)&lt;/small&gt; &lt;br/&gt; &lt;small&gt;(Leave EMPTY for -ALL-)&lt;/small&gt;" description="Enter Fields (their fieldnames) to be included in text searching. You can separate each fieldname by comma(,). NOTE that fields not marked as (Search View) Text Searchable will be skipped even if you add them here" cols="30" rows="3" />
			
			<field name="" type="separator" level="level2" default="Content types" description="The content types the search will include, selecting none will include ALL. NOTE: You can allow user to select Content Types. If you do then the selection list will be LIMITED to display ONLY the types specified here"/>
			
			<field name="canseltypes" type="list" default="1" label="User selection" description="Allow users to select content types they want to include in their search">
				<option value="0">No, Use all Content Types</option>
				<option value="1">Yes, Always</option>
				<option value="2">Yes, as Advanced Option</option>
			</field>
			<field name="contenttypes" type="types" default="" label="Searchable Types &lt;br/&gt; &lt;small&gt;(Select NONE for -ALL-)&lt;/small&gt;" description="The content types the search will include, selecting none will include ALL. NOTE: You can allow user to select Content Types. If you do then the selection list will be LIMITED to display ONLY the types specified here" multiple="true" />
			
			
