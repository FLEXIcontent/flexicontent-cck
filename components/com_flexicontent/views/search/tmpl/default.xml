				<option value="0">FLEXI_HIDE</option>
				<option value="1">FLEXI_SHOW</option>
			</field>
			<field name="default_filtersop" type="radio" default="all" label="Default combination" description="Default (preselected) method of combining the search filters">
				<option value="OR">FLEXI_ANY</option>
				<option value="AND">FLEXI_ALL</option>
			</field-->
			
			<field name="filters_per_type" type="radio" default="1" label="FLEXI_FILTERS_ACCORDING_TYPE" description="FLEXI_FILTERS_ACCORDING_TYPE_DESC">
				<option value="0">FLEXI_NO_USE_BELOW</option>
				<option value="1">FLEXI_YES</option>
			</field>
			<field name="filters_order" type="radio" default="1" label="FLEXI_FILTERS_ORDER" description="FLEXI_FILTERS_ORDER_DESC">
