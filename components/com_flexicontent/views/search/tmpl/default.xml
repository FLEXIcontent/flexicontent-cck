		<description>STANDARD SEARCH LAYOUT DESC</description>
		<params addpath="/administrator/components/com_flexicontent/elements">
		
		<field name="" type="separator" level="level3" default="Please note these reasons for some results not being included:&lt;small&gt; &lt;br/&gt;- word is less than 4 characters (default for MySql DBs)&lt;br/&gt;- word is too common e.g. appears in about half of the articles&lt;br/&gt;- wrong language or article not publish or article has expired&lt;/small&gt;" />
		
		<field name="" type="separator" level="level1" default="Search Form: &lt;small&gt;Basic Options&lt;/small&gt;" />
			
			<field name="" type="separator" level="level2" default="Text Search" />
			
			<field name="" type="separator" level="level3" default="Words of Text Search" />
			 
			<field name="show_search_label" type="list" default="" label="FLEXI_TEXT_SEARCH_LABEL" description="FLEXI_TEXT_SEARCH_LABEL_DESC" >
				<option value="">FLEXI_USE_GLOBAL</option>
				<option value="0">FLEXI_HIDE</option>
				<option value="1">FLEXI_OUTSIDE_INPUT</option>
				<option value="2">FLEXI_INSIDE_INPUT</option>
			</field>
			<field name="show_searchphrase" type="radio" default="1" label="Word Combination" description="Allow users to select method of combining the search words, e.g. phrase, phrase with guessing relevant, all, any, exact phrase">
				<option value="0">FLEXI_HIDE</option>
				<option value="1">FLEXI_SHOW</option>
			</field>
			<field name="default_searchphrase" type="list" default="all" label="Default combination" description="Default (preselected) method of combining the search words">
