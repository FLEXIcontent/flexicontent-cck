				<option value="1">FLEXI_YES_INITIALLY_DISABLED</option>
				<option value="2">FLEXI_YES_INITIALLY_ENABLED</option>
			</field>
			
			<field name="" type="separator" level="level2" default="Fields as Filters" />
			
